#include <iostream>
#include <cmath>

using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
	cout << "Пожалуйста, введите числа А и Х в соответствии с условиями операций над числами:\n1) sqrt(ln|A^2 + X^3|) при -2<=AX<=2 или 4 <= AX <= 6\n2) X^3 * exp^(A*X) при -7 < AX < 0\n3) sqrt(A + |A * X|) при AX < -2 или AX > 6\n";
	float Y, X, A;
	cin >> A >> X;
	if (A * X >= -2 and A * X <= 2 or A * X >= 4 and A * X <= 6) {
		Y = sqrt(log(abs(pow(A, 2) + pow(X, 3))));
	}
	else if (A * X > -7 and A * X < 0) {
		Y = pow(X, 3) * exp(A * X);
	}
	else if (A * X < -2 or A * X > 6) {
		Y = sqrt(A + abs(A * X));
	}
	cout << Y << endl;
	system("PAUSE");
}
